public class prime {

    public static void main(String args[]){
		int a = Integer.parseInt("5");
		int b=0;
		for(int i=2;i<a;i++){
			if(a%i==0)
			{
				System.out.println(a+" is not a Prime Number");
				b = 1;
				break;
			}
		}
		if(b==0)
			System.out.println(a+" is a Prime Number");
	}
}
